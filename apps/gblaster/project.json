{
  "name": "gblaster",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "generators": {
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "sourceRoot": "apps/gblaster/src",
  "prefix": "mtb",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "allowedCommonJsDependencies": [
          "music-metadata-browser",
          "music-metadata/lib/core",
          "debug",
          "readable-web-to-node-stream",
          "typedarray-to-buffer",
          "node-vibrant/lib/browser",
          "lucene-escape-query"
        ],
        "outputPath": "dist/apps/gblaster",
        "index": "apps/gblaster/src/index.html",
        "main": "apps/gblaster/src/main.ts",
        "polyfills": "apps/gblaster/src/polyfills.ts",
        "tsConfig": "apps/gblaster/tsconfig.app.json",
        "assets": ["apps/gblaster/src/favicon.ico", "apps/gblaster/src/assets", "apps/gblaster/src/manifest.webmanifest"],
        "styles": ["apps/gblaster/src/styles/theme.scss", "apps/gblaster/src/styles/main.scss"],
        "scripts": [],
        "webWorkerTsConfig": "apps/gblaster/tsconfig.worker.json",
        "vendorChunk": true,
        "extractLicenses": false,
        "buildOptimizer": false,
        "sourceMap": true,
        "optimization": false,
        "namedChunks": true
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/gblaster/src/environments/environment.ts",
              "with": "apps/gblaster/src/environments/environment.prod.ts"
            }
          ],
          "optimization": {
            "scripts": true,
            "fonts": {
              "inline": true
            },
            "styles": {
              "minify": true,
              "inlineCritical": true
            }
          },
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "10kb"
            }
          ],
          "serviceWorker": true,
          "ngswConfigPath": "apps/gblaster/ngsw-config.json"
        }
      },
      "outputs": ["{options.outputPath}"]
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "browserTarget": "gblaster:build"
      },
      "configurations": {
        "production": {
          "browserTarget": "gblaster:build:production"
        }
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "gblaster:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/gblaster/src/**/*.ts", "apps/gblaster/src/**/*.html"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "apps/gblaster/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["{workspaceRoot}/coverage/apps/gblaster"]
    },
    "deploy": {
      "executor": "@angular/fire:deploy",
      "options": {}
    },
    "stylelint": {
      "executor": "nx-stylelint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/gblaster/**/*.css", "apps/gblaster/**/*.scss"]
      }
    },
    "compodoc": {
      "executor": "@twittwer/compodoc:compodoc",
      "options": {
        "tsConfig": "apps/gblaster/tsconfig.editor.json",
        "outputPath": "dist/compodoc/gblaster"
      },
      "configurations": {
        "json": {
          "exportFormat": "json"
        }
      }
    }
  },
  "tags": ["app"]
}
