{
  "projectType": "library",
  "root": "libs/core/theme",
  "sourceRoot": "libs/core/theme/src",
  "prefix": "mtb",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/core/theme/src/**/*.ts", "libs/core/theme/src/**/*.html"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/core/theme/jest.config.js",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/core/theme"]
    }
  },
  "generators": {
    "@nrwl/angular:component": {
      "styleext": "scss"
    }
  },
  "tags": ["lib", "angular", "core"]
}
