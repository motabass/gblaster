{
  "projectType": "library",
  "root": "libs/helper-services/media-session",
  "sourceRoot": "libs/helper-services/media-session/src",
  "prefix": "mtb",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/helper-services/media-session/src/**/*.ts", "libs/helper-services/media-session/src/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/helper-services/media-session/jest.config.js",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/helper-services/media-session"]
    }
  },
  "generators": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "tags": []
}
