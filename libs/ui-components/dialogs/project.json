{
  "projectType": "library",
  "root": "libs/ui-components/dialogs",
  "sourceRoot": "libs/ui-components/dialogs/src",
  "prefix": "mtb",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/ui-components/dialogs/src/**/*.ts", "libs/ui-components/dialogs/src/**/*.html"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/ui-components/dialogs/jest.config.js",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/ui-components/dialogs"]
    }
  },
  "generators": {
    "@nrwl/angular:component": {
      "styleext": "scss"
    }
  },
  "tags": ["lib", "component"]
}
