{
  "projectType": "library",
  "root": "libs/ui-components/visuals",
  "sourceRoot": "libs/ui-components/visuals/src",
  "prefix": "mtb",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "options": {
        "tsConfig": "libs/ui-components/visuals/tsconfig.lib.json",
        "project": "libs/ui-components/visuals/ng-package.json",
        "buildableProjectDepsInPackageJsonType": "dependencies"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ui-components/visuals/tsconfig.lib.prod.json"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/ui-components/visuals/src/**/*.ts", "libs/ui-components/visuals/src/**/*.html"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/ui-components/visuals/jest.config.js",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/ui-components/visuals"]
    },
    "stylelint": {
      "executor": "nx-stylelint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/ui-components/visuals/**/*.css", "libs/ui-components/visuals/**/*.scss"]
      }
    }
  },
  "generators": {
    "@nrwl/angular:component": {
      "styleext": "scss"
    }
  },
  "tags": ["lib", "component"]
}
