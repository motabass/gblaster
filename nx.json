    },
    "tslint.json": "*",
    "nx.json": "*",
    "tsconfig.base.json": "*"
  },
  "projects": {
    "core-theme": {
      "tags": ["lib", "angular", "core"]
    },
    "core-update": {
      "tags": ["lib", "angular", "core"]
    },
    "gblaster": {
      "tags": ["app"]
