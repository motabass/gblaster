    "helper-services-gamepad": {
      "tags": ["lib", "angular", "helper"]
    },
    "helper-services-hotkeys": {
      "tags": ["lib", "angular", "helper"]
    },
    "helper-services-loader": {
      "tags": ["lib", "angular", "helper"]
    },
    "helper-services-media-session": {
      "tags": []
    },
    "helper-services-title": {
      "tags": ["lib", "angular", "helper"]
    },
    "helper-services-wakelock": {
      "tags": []
    },
    "helpers-memoization": {
      "tags": ["lib", "ts", "helper"]
    },
    "helpers-time": {
      "tags": ["lib", "ts", "helper"]
    },
    "helpers-unsubscription": {
      "tags": ["lib", "angular", "helper"]
    },
    "material-helpers-mat-icon-size": {
      "tags": ["lib", "angular", "helper"]
    },
    "player": {
      "tags": ["lazy-module"]
    },
    "settings": {
      "tags": []
    },
    "ui-components-dialogs": {
      "tags": ["lib", "component"]
    },
    "ui-components-file-drop-overlay": {
      "tags": ["lib", "component"]
    },
    "ui-components-slide-panel": {
      "tags": ["lib", "component"]
    },
    "ui-components-slide-panel-e2e": {
      "tags": ["e2e"],
      "implicitDependencies": ["ui-components-slide-panel"]
    },
    "ui-components-visuals": {
      "tags": ["lib", "component"]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e", "build-storybook"]
      }
    }
  },
  "affected": {
    "defaultBase": "master"
  }
