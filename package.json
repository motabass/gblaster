{
  "name": "motabass",
  "version": "4.4.0",
  "author": "motabass",
  "description": "nx monorepo for my web stuff",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "npm run serve:gblaster",
    "serve:gblaster": "nx serve gblaster",
    "build": "nx build gblaster",
    "build:prod": "nx build --prod",
    "build:prod:gblaster": "nx build gblaster --prod",
    "build:visuals": "nx build ui-components-visuals --prod",
    "publish:visuals": "npm run build:visuals && cd dist/libs/ui-components/visuals/ && npm publish --access public",
    "test": "ng test",
    "storybook:build": "nx run ui-components-slide-panel:build-storybook",
    "storybook": "nx run ui-components-slide-panel:storybook",
    "deploy": "ng deploy",
    "lint": "nx workspace-lint & ng lint --fix",
    "lint:styles": "stylelint \"{apps,libs}/**/*.{scss,html,ts}\" --fix",
    "lint:precommit": "tslint --project tsconfig.base.json --config tslint.precommit.json --fix --force",
    "e2e": "nx e2e",
    "e2e:watch": "nx e2e gblaster-e2e --watch",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "prettier": "prettier --config ./.prettierrc --write \"{apps,libs,tools,.github}/**/*.{scss,html,ts,js,json,md,yml,yaml,css}\"",
    "upgrade": "ncu -ui",
    "update:check": "ng update",
    "update:nx": "ng update @nrwl/workspace --allow-dirty --force",
    "update:cli": "ng update @angular/cli --allow-dirty --force",
    "update:angular": "ng update @angular/core --allow-dirty --force",
    "update:material": "ng update @angular/material --allow-dirty --force",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "generate:ui-lib": "ng generate @nrwl/angular:library --name=test --style=scss --directory=ui-components --prefix=mtb --publishable --simpleModuleName",
    "generate:lazy-module-lib": "ng generate @nrwl/angular:library --name=test --style=scss --directory=ui-components --prefix=hsr",
    "assets:optimize:svg": "svgo -f apps/gblaster/src/assets --disable=removeViewBox",
    "bundle-analyze:build:gblaster": "npm run build:prod -- --stats-json --named-chunks && npm run bundle-analyze:gblaster",
    "bundle-analyze:gblaster": "webpack-bundle-analyzer dist/apps/gblaster/stats.json --port 8889",
    "bundle-analyze:static:gblaster": "webpack-bundle-analyzer dist/apps/gblaster/stats.json --mode static --report docs/analyzer-report.html --no-open",
    "bundle-analyze:build:static:gblaster": "npm run build:prod -- --stats-json --named-chunks && webpack-bundle-analyzer dist/apps/gblaster/stats.json --mode static --report docs/analyzer-report.html --no-open",
    "release": "bump minor --tag --commit --push --no-verify -a && git checkout release && git merge master --no-verify && git push && git checkout master",
    "release:major": "bump major --tag --commit --push --no-verify -a && git checkout release && git merge master --no-verify && git push && git checkout master",
    "release:patch": "bump patch --tag --commit --push --no-verify -a && git checkout release && git merge master --no-verify && git push && git checkout master",
    "ci:build": "npm run build:prod:gblaster",
    "ci:e2e": "nx run gblaster-e2e:e2e --headless",
    "ci:deploy": "nx deploy gblaster",
    "compodoc": "compodoc -p tsconfig.compodoc.json -d docs/compodoc",
    "compodoc:serve": "npm run compodoc -- -s",
    "generate:icon-set": "ts-node --project tools/tsconfig.tools.json tools/convertIcons.ts",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && node ./decorate-angular-cli.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:precommit"
    ]
  },
  "dependencies": {
    "@allex/crc32": "^1.0.4",
    "@angular/animations": "^10.2.0",
    "@angular/cdk": "^10.2.5",
    "@angular/common": "^10.2.0",
    "@angular/compiler": "^10.2.0",
    "@angular/core": "^10.2.0",
    "@angular/fire": "^6.0.3",
    "@angular/flex-layout": "10.0.0-beta.32",
    "@angular/forms": "^10.2.0",
    "@angular/localize": "10.2.0",
    "@angular/material": "^10.2.5",
    "@angular/platform-browser": "^10.2.0",
    "@angular/platform-browser-dynamic": "^10.2.0",
    "@angular/router": "^10.2.0",
    "@angular/service-worker": "^10.2.0",
    "@thebespokepixel/es-tinycolor": "^2.1.1",
    "browser-id3-writer": "^4.4.0",
    "buffer": "^5.6.0",
    "core-js": "^3.6.5",
    "d3": "^6.2.0",
    "date-fns": "^2.16.1",
    "fast-memoize": "^2.5.2",
    "firebase": "^7.24.0",
    "jsmediatags": "^3.9.3",
    "lucene-escape-query": "^1.0.1",
    "material-community-components": "^9.0.3",
    "mobx": "^5.15.7",
    "mobx-angular": "^4.1.0",
    "music-metadata-browser": "2.1.5",
    "ngx-date-fns": "6.5.0",
    "ngx-indexed-db": "^6.1.0",
    "ngx-pipes": "2.7.5",
    "ngx-webstorage": "6.0.0",
    "node-vibrant": "^3.1.6",
    "process": "^0.11.10",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "^0.11.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1002.0",
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "^10.2.0",
    "@babel/core": "7.12.3",
    "@compodoc/compodoc": "^1.1.11",
    "@nrwl/angular": "10.3.1",
    "@nrwl/cli": "10.3.1",
    "@nrwl/cypress": "10.3.1",
    "@nrwl/jest": "10.3.1",
    "@nrwl/node": "10.3.1",
    "@nrwl/storybook": "10.3.1",
    "@nrwl/workspace": "10.3.1",
    "@storybook/addon-knobs": "6.0.26",
    "@storybook/angular": "6.0.26",
    "@storybook/react": "6.0.26",
    "@types/d3": "^5.16.3",
    "@types/jest": "26.0.15",
    "@types/jsmediatags": "~3.9.1",
    "@types/node": "~14.14.2",
    "@types/prettier": "^2.1.5",
    "@types/svgo": "^1.3.3",
    "@types/tinycolor2": "^1.4.2",
    "@types/wicg-file-system-access": "^2020.9.1",
    "babel-loader": "8.1.0",
    "codelyzer": "~6.0.1",
    "colors": "1.4.0",
    "cypress": "5.4.0",
    "cypress-file-upload": "^4.1.1",
    "dotenv": "8.2.0",
    "eslint": "7.11.0",
    "eslint-plugin-cypress": "^2.11.2",
    "firebase-tools": "^8.13.1",
    "fuzzy": "^0.1.3",
    "husky": "^4.3.0",
    "inquirer": "^7.3.3",
    "inquirer-autocomplete-prompt": "^1.2.0",
    "jest": "26.6.0",
    "jest-preset-angular": "8.3.1",
    "lint-staged": "^10.4.2",
    "ng-packagr": "^10.1.2",
    "npm-check-updates": "9.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.2",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "simple-git": "2.21.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0",
    "svgo": "^1.3.2",
    "ts-jest": "26.4.1",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "tslint-clean-code": "^0.2.10",
    "tslint-config-prettier": "^1.18.0",
    "tslint-etc": "^1.13.7",
    "tslint-sonarts": "1.9.0",
    "typescript": "~4.0.3",
    "version-bump-prompt": "^6.1.0",
    "wait-on": "~5.2.0",
    "webpack-bundle-analyzer": "3.9.0"
  }
}
