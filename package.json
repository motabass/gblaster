{
  "name": "motabass",
  "version": "8.1.7",
  "author": "motabass",
  "repository": "https://github.com/motabass/motabass",
  "description": "nx monorepo for my web stuff",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "npm run serve:gblaster",
    "serve:gblaster": "nx serve gblaster",
    "build": "nx build gblaster",
    "build:prod": "nx build --prod",
    "build:prod:gblaster": "nx build gblaster --prod",
    "build:visuals": "nx build ui-components-visuals --prod",
    "publish:visuals": "npm run build:visuals && cd dist/libs/ui-components/visuals/ && npm publish --access public",
    "test": "ng test",
    "storybook:build": "nx run ui-components-slide-panel:build-storybook",
    "storybook": "nx run ui-components-slide-panel:storybook",
    "deploy": "ng deploy",
    "lint": "nx workspace-lint & ng lint --fix",
    "lint:styles": "stylelint \"{apps,libs}/**/*.{scss,html,ts}\" --fix",
    "lint:precommit": "tslint --project tsconfig.base.json --config tslint.precommit.json --fix --force",
    "e2e": "nx e2e gblaster-e2e",
    "e2e:watch": "nx e2e gblaster-e2e --watch",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "prettier": "prettier --config ./.prettierrc --write \"{apps,libs,tools,.github}/**/*.{scss,html,ts,js,json,md,yml,yaml,css}\"",
    "upgrade": "ncu -ui",
    "update": "nx migrate latest",
    "migrate": "nx migrate --run-migrations=migrations.json",
    "update:cli": "ng update @angular/cli --allow-dirty --force",
    "update:angular": "ng update @angular/core --allow-dirty --force",
    "update:material": "ng update @angular/material --allow-dirty --force",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "generate:lib:ui-component": "ng generate @nrwl/angular:library --name=test --style=scss --directory=ui-components --prefix=mtb --publishable --simpleModuleName",
    "generate:lib:helper-service": "ng generate @nrwl/angular:library --name=media-session --style=scss --directory=helper-services --prefix=mtb --simpleModuleName",
    "generate:lazy-module-lib": "ng generate @nrwl/angular:library --name=test --style=scss --directory=ui-components --prefix=hsr",
    "assets:optimize:svg": "svgo -f apps/gblaster/src/assets --disable=removeViewBox",
    "bundle-analyze:build:gblaster": "npm run build:prod -- --stats-json --named-chunks && npm run bundle-analyze:gblaster",
    "bundle-analyze:gblaster": "webpack-bundle-analyzer dist/apps/gblaster/stats.json --port 8889",
    "bundle-analyze:static:gblaster": "webpack-bundle-analyzer dist/apps/gblaster/stats.json --mode static --report docs/analyzer-report.html --no-open",
    "bundle-analyze:build:static:gblaster": "npm run build:prod -- --stats-json --named-chunks && webpack-bundle-analyzer dist/apps/gblaster/stats.json --mode static --report docs/analyzer-report.html --no-open",
    "version": "bump minor --tag --commit --push --no-verify -a",
    "version:major": "bump major --tag --commit --push --no-verify -a",
    "release:interactive": "dotenv release-it",
    "release": "dotenv release-it -- --ci",
    "ci:build": "npm run build:prod:gblaster",
    "ci:e2e": "nx run gblaster-e2e:e2e --headless",
    "ci:deploy": "nx deploy gblaster",
    "compodoc": "compodoc -p tsconfig.compodoc.json -d docs/compodoc",
    "compodoc:serve": "npm run compodoc -- -s",
    "generate:icon-set": "ts-node --project tools/tsconfig.tools.json tools/convertIcons.ts",
    "postinstall": "ngcc --properties es2015 browser module main && node ./decorate-angular-cli.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:precommit"
    ]
  },
  "release-it": {
    "github": {
      "release": true
    },
    "hooks": {
      "after:bump": "npx auto-changelog -p",
      "after:release": "echo Successfully released ${name} v${version} to ${repo.repository}."
    },
    "git": {
      "requireCleanWorkingDir": false,
      "requireBranch": "master",
      "changelog": "npx auto-changelog --stdout --commit-limit false --unreleased --template https://raw.githubusercontent.com/release-it/release-it/master/templates/changelog-compact.hbs"
    },
    "npm": {
      "publish": false
    }
  },
  "dependencies": {
    "@allex/crc32": "^1.0.4",
    "@angular/animations": "12.1.1",
    "@angular/cdk": "^12.1.1",
    "@angular/common": "12.1.1",
    "@angular/compiler": "12.1.1",
    "@angular/core": "12.1.1",
    "@angular/fire": "^6.1.5",
    "@angular/flex-layout": "12.0.0-beta.34",
    "@angular/forms": "12.1.1",
    "@angular/localize": "12.1.1",
    "@angular/material": "^12.1.1",
    "@angular/platform-browser": "12.1.1",
    "@angular/platform-browser-dynamic": "12.1.1",
    "@angular/router": "12.1.1",
    "@angular/service-worker": "12.1.1",
    "@thebespokepixel/es-tinycolor": "^2.1.1",
    "browser-id3-writer": "^4.4.0",
    "buffer": "^6.0.3",
    "core-js": "^3.15.2",
    "d3": "^7.0.0",
    "date-fns": "^2.22.1",
    "fast-memoize": "^2.5.2",
    "firebase": "^8.7.1",
    "jsmediatags": "^3.9.7",
    "lucene-escape-query": "^1.0.1",
    "material-community-components": "^10.0.0",
    "mobx": "5.15.7",
    "mobx-angular": "4.2.0",
    "music-metadata-browser": "2.2.7",
    "ngx-date-fns": "8.1.0",
    "ngx-indexed-db": "^9.1.2",
    "ngx-pipes": "2.7.5",
    "ngx-webstorage": "8.0.0",
    "node-vibrant": "^3.1.6",
    "process": "^0.11.10",
    "rxjs": "~7.2.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1201.1",
    "@angular-devkit/build-angular": "12.1.1",
    "@angular-eslint/eslint-plugin": "~12.2.0",
    "@angular-eslint/eslint-plugin-template": "~12.2.0",
    "@angular-eslint/template-parser": "~12.2.0",
    "@angular/cli": "12.1.1",
    "@angular/compiler-cli": "12.1.1",
    "@angular/language-service": "12.1.1",
    "@babel/core": "7.14.6",
    "@compodoc/compodoc": "^1.1.13",
    "@nrwl/angular": "12.5.7",
    "@nrwl/cli": "12.5.7",
    "@nrwl/cypress": "12.5.7",
    "@nrwl/eslint-plugin-nx": "12.5.7",
    "@nrwl/jest": "12.5.7",
    "@nrwl/linter": "12.5.7",
    "@nrwl/node": "12.5.7",
    "@nrwl/storybook": "12.5.7",
    "@nrwl/workspace": "12.5.7",
    "@storybook/addon-essentials": "~6.3.4",
    "@storybook/addon-knobs": "~6.3.0",
    "@storybook/angular": "^6.3.4",
    "@storybook/builder-webpack5": "~6.3.4",
    "@storybook/manager-webpack5": "~6.3.4",
    "@storybook/react": "^6.3.4",
    "@types/d3": "^7.0.0",
    "@types/dom-screen-wake-lock": "^1.0.0",
    "@types/jest": "26.0.24",
    "@types/jsmediatags": "~3.9.3",
    "@types/node": "14.14.41",
    "@types/prettier": "^2.3.2",
    "@types/svgo": "^2.3.1",
    "@types/tinycolor2": "^1.4.3",
    "@types/wicg-file-system-access": "^2020.9.2",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "babel-loader": "8.2.2",
    "colors": "1.4.0",
    "conventional-changelog-cli": "^2.1.1",
    "cypress": "7.7.0",
    "cypress-file-upload": "^5.0.8",
    "dotenv": "10.0.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "eslint-plugin-react": "latest",
    "firebase-tools": "^9.16.0",
    "fuzzy": "^0.1.3",
    "husky": "^7.0.1",
    "inquirer": "^8.1.1",
    "inquirer-autocomplete-prompt": "^1.4.0",
    "jest": "27.0.6",
    "jest-preset-angular": "9.0.4",
    "lint-staged": "^11.0.0",
    "ng-packagr": "12.1.1",
    "npm-check-updates": "11.8.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.3.2",
    "pretty-quick": "^3.1.1",
    "release-it": "14.10.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-scss": "^3.19.0",
    "svgo": "^2.3.1",
    "ts-jest": "27.0.3",
    "tslint": "^6.1.3",
    "ts-node": "10.0.0",
    "tslint-etc": "^1.13.10",
    "typescript": "next",
    "wait-on": "~6.0.0",
    "webpack-bundle-analyzer": "4.4.2"
  }
}
